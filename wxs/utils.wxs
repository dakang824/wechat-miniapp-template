/* eslint-disable */
var bem = require("./bem.wxs");
var memoize = require("./memoize.wxs");
var addUnit = require("./add-unit.wxs");

var includes = function (arr, val) {
  return (
    arr.filter(function (item) {
      return item === val;
    }).length > 0
  );
};

var zeroFill = function (val) {
  return val < 10 ? "0" + val : val;
};

module.exports = {
  bem: memoize(bem),
  memoize: memoize,
  addUnit: addUnit,
  includes: includes,
  zeroFill: zeroFill,
};
