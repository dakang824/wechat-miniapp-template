<container>
  <block wx:if="{{list.length}}">
    <list-item wx:for="{{ list }}" wx:key="index" mode="{{ item.article }}"></list-item>
    <load-more status="{{status}}" length='{{list.length}}'/>
  </block>
  <block wx:else>
    <van-empty description="~ 暂无数据 ~" />
  </block>
</container>
